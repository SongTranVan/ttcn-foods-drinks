<tr>
  <td><%= user.id %></td>
  <td><%= link_to user.name, user %></td>
  <td><%= user.email %></td>
  <td>
    <%= link_to t("edit"), edit_user_path(user)  if current_user? user %>
    <%= link_to t("delete"), admin_user_path(user), method: :delete, data: {confirm: t("you_sure")} if can_delete? user %>
  </td>
</tr>
